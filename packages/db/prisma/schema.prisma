
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String       @id @default(uuid())
  googleId     String?       @unique
  email        String       @unique
  password     String?
  name         String
  username     String?       @unique 
  avatar       String?
  createdAt    DateTime     @default(now())    
}